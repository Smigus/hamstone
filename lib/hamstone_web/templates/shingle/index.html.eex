  <body>
    <nav class="navbar navbar-nav navbar-dark fixed-top bg-dark flex-md-nowrap p-0 shadow">
      <div id="container" style="margin: 0 auto; min-width: 200px;">
        <div id="container" style="inner-margin: 5px;">
          <img src="<%= Routes.static_path(@conn, "/images/Hambat.png") %>" class="rounded float-left" style="width: 50px;">
        </div>
        <a class="display-4 text-white text-center text-decoration-none" href=<%= Routes.page_path(@conn, :index) %>>Hamstone</a>
      </div>
    </nav>

<div class="container-fluid">
  <div class="row">
    <main role="main" class="col ml-sm-auto px-4">

      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Dashboard</h1>
      </div>

      <div id="container" style="min-width: 310px; height: 300px; margin: 0 auto">

        <div class="row"  style="height: 100px;"></div>
        <div class="row justify-content-center">
          <h2 class="display-2 text-center">#<%= @shingle_count%> Shingles in total</h2>
        </div>
      </div>

      <div class="container">
        <div class="row" style="margin: 0 auto">
          <div class="col w-100" style="margin: 0 auto">
            <nav class="navbar navbar-expand-lg navbar-white">
              <div class="row" style="margin: 0 auto">
                <ul class="navbar-nav w-100">
                  <li class="col-11"></li>
                  
                </ul>
              </div>
            </nav>
          </div>
        </div>
      </div>

      <div class="table">
        <table class="table table-striped">
          <thead>
            <tr>
              <th scope="col">Shingles</th> 
              <th scope="col"></th>
              <th scope="col"></th>
            </tr>
          </thead>
          <tbody>
            <%= for i <- 0..32 do %>
              <tr>
                <td>
                    <div class="row">
                    <div class="col"> <input type="checkbox" class="form-check-input" id="exampleCheck1"> </div>
                    <div class="col"><%= "#{Enum.at(@shingles, i * 3).word1} #{Enum.at(@shingles, i * 3).word2} #{Enum.at(@shingles, i * 3).word3}" %></div>4
                    </div>
                </td>
                <td> <%= "#{Enum.at(@shingles, i * 3 + 1).word1} #{Enum.at(@shingles, i * 3 + 1).word2} #{Enum.at(@shingles, i * 3 + 1).word3}" %> </td>
                <td> <%= "#{Enum.at(@shingles, i * 3 + 2).word1} #{Enum.at(@shingles, i * 3 + 2).word2} #{Enum.at(@shingles, i * 3 + 2).word3}" %> </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>

        <script src="<%= Routes.static_path(@conn, "/js/checkbox.js") %>"> </script>
